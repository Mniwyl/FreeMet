<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:FreeMet2.Theme.Light">
   
    <!--字体颜色-->
    <SolidColorBrush x:Key="ForeColor" Color="White"></SolidColorBrush>
    
    <!--控件背景颜色-->
    <SolidColorBrush x:Key="BackColor" Color="CornflowerBlue"/>
    <SolidColorBrush x:Key="BorderColor" Color="CornflowerBlue"/>
    <SolidColorBrush x:Key="MainFormBackColor" Color="WhiteSmoke"/>
</ResourceDictionary>
